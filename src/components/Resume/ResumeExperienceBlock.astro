---
import Collapser from "../Collapser.vue";
import { render } from "astro:content";

const { entry } = Astro.props;

const { Content } = await render(entry);
---

<Collapser
  client:load
  title-wrapper="h4"
  title={entry.data.title}
  subtitle={entry.data.subtitle}
>
  <Content />
</Collapser>
